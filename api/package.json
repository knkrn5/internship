    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.7.0",
    "@types/nodemailer": "^7.0.2",
    "eslint": "^9.37.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
